            "type": "text",
      "id": "resorce_second_head_text_third",
      "label": "heading"
         },
      {
            "type": "html",
      "id": "resorce_second_third_text",
      "label": "text"
         },
      {
            "type": "text",
      "id": "resorce_second_third_cust_pera_text",
      "label": "peragraph"
         },
       {
            "type": "text",
      "id": "resorce_second_text",
      "label": "peragraph 2"
         },
       {
            "type": "text",
      "id": "third_resorce_second_pera_text",
      "label": "peragraph 3"
         },
       {
            "type": "text",
      "id": "third_resorce_second_text",
      "label": "peragraph 4"
         }
      
    ],
      "blocks": [
     {
        "name": "points text",
       "type": "text",
       "settings": [
        
{
 			"type": "text",
 			"id": "third_resorce_pera_text_sect",
 			"label": "Text"
 		}
       ]
     }
  ],
    "presets": [
      {
        "name":"resorce custom section"
      }
    ]
